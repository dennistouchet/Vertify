{
	"$schema": "http://json-schema.org/schema#",
	"id": "http://vertify.hellodata.com/agent.json",
	"title": "agent",
	"description": "This is the Agent schema.",
	"agent": {
		"type": "object",
		"properties": {
			"tenant_id": {
				"type": "intenger"
			},
			"id": {
				"type": "string"
			},
			"modified": {
				"type": "string",
				"format": "date-time"
			},
			"created": {
				"type": "string",
				"format": "date-time"
			},
			"is_deleted": {
				"type": "boolean"
			},
			"last_poll": {
				"type": "string",
				"format": "date-time"
			},
			"instance_start": {
				"type": "string",
				"format": "date-time"
			},
			"instance_end": {
				"type": "string",
				"format": "date-time"
			},
			"instace_id": {
				"type": "integer"
			},
			"current_load": {
				"type": "integer"
			},
			"status": {
				"type": "string",
				"enum": ["started", "stopped", "unresponsive"]
			},
			"version": {
				"type": "string"
			},
			"installer_computer_name": {
				"type": "string"
			},
			"installer_id": {
				"type": "string"
			},
			"external_ip": {
				"type": "string"
			},
			"internal_ip": {
				"type": "string"
			},
			"name": {
				"type": "string"
			},
			"last_login": {
				"type": "string",
				"format": "date-time"
			}
		}
	},
	"required": ["tenant_id", "id", "modified", "created",
		"is_deleted", "last_poll", "instance_start",
		"instance_end", "instance_id", "current_load",
		"status", "version", "installer_computer_name",
		"installer_id", "external_ip", "internal_ip",
		"name", "last_login"
	]
}
